nsive=\"sm\">\n                        <template v-slot:cell(action)=\"row\">\n                            <b-button size=\"sm\"  variant=\"outline-primary\" class=\"mr-2\" @click=\"edit(row.index)\">\n                                编辑\n                            </b-button>\n                            <b-button size=\"sm\"  variant=\"danger\" class=\"mr-2\" @click=\"del(row.index)\">\n                               删除\n                            </b-button>\n\n\n                        </template>\n                    </b-table>\n\n                    <!--          分页-->\n                    <b-pagination\n\n                            v-model=\"currentPage\"\n                            :total-rows=\"rows\"\n                            :per-page=\"perPage\"\n                            aria-controls=\"my-table\"\n                    ></b-pagination>\n\n\n\n                </b-col>\n            </b-row>\n        </b-container>\n\n\n    </div>\n</template>\n\n<script>\n    import axios from 'axios';\n    import TopNav from '../components/top-nav';\n    import LeftNav from '../components/left-nav';\n    import qs from 'qs';\n\n    export default {\n        name:'Product_read',\n        props:[''],\n        data () {\n            return{\n                items: [\n\n                ],\n                fields: ['id','name', 'content', 'sale_num','status','sort','action'],\n                perPage: 5,\n                currentPage: 1,\n            }\n        },\n\n        components: {\n            TopNav,\n            LeftNav,\n\n        },\n\n        computed: {\n            rows() {\n                return this.items.length\n            }\n        },\n\n        beforeMount() {},\n\n        mounted() {\n            axios({\n                method: 'get',\n                url: 'http://127.0.0.1:8080/laravel/public/api/product',\n                // data: qs.stringify(this.data),\n                headers: { 'Content-Type':'application/x-www-form-urlencoded' },\n            })\n                .then((res)=>{\n                    // console.log(res);\n\n                    this.items = res.data.data;\n                    console.log(this.items)\n                })\n                .catch(function(error){\n                    console.log(error)//错误处理 相当于error\n                })\n        },\n\n        methods: {\n            edit(a){\n                let id = this.items[a].id;\n                // console.log(id);\n                // axios({\n                //     method: 'get',\n                //     url: 'http://127.0.0.1:8080/laravel/public/api/product',\n                //     // data: qs.stringify(this.data),\n                //     headers: { 'Content-Type':'application/x-www-form-urlencoded' },\n                // })\n                //     .then((res)=>{\n                //         // console.log(res);\n                //\n                //         this.items = res.data.data;\n                //         console.log(this.items)\n                //     })\n                //     .catch(function(error){\n                //         console.log(error)//错误处理 相当于error\n                //     })\n            },\n            del(a){\n                let id = this.items[a].id;\n\n                axios({\n                    method: 'get',\n                    url: 'http://127.0.0.1:8080/laravel/public/api/product/del?id='+id,\n                    // data: qs.stringify(this.data),\n                    headers: { 'Content-Type':'application/x-www-form-urlencoded' },\n                })\n                    .then((res)=>{\n                        console.log(res);\n                        if (res.data.code == 0){\n                            location. reload()\n                        }\n\n                    })\n                    .catch(function(error){\n                        console.log(error)//错误处理 相当于error\n                    })\n            }\n        },\n\n        watch: {}\n\n    }\n\n\n</script>\n<style scoped lang=\"scss\">\n\n    body,ol,ul,h1,h2,h3,h4,h5,h6,p,th,td,dl,dd,form,fieldset,legend,input,textarea,select{margin:0;padding:0}\n    body{font:12px\"宋体\",\"Arial Narrow\",HELVETICA;background:#fff;-webkit-text-size-adjust:100%;}\n    a{color:#2d374b;text-decoration:none}\n    a:hover{color:#cd0200;text-decoration:underline}\n    em{font-style:normal}\n    li{list-style:none}\n    img{border:0;vertical-align:middle}\n    table{border-collapse:collapse;border-spacing:0}\n    p{word-wrap:break-word}\n\n\n    .caidan{\n        background-color: #718498;\n        height: 90vh;\n    }\n    .edit{\n        background-color: #718498;\n        color: white;\n    }\n    .del{\n        background-color: #42b983;\n        color: white;\n    }\n\n\n\n\n\n</style>\n"]}]}